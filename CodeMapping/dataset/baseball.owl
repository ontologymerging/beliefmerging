    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
  xml:base="http://www.workingontologist.org/Examples/Chapter10/baseball.owl">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="CarpPlayer">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Carp player</rdfs:label>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="playsFor"/>
        </owl:onProperty>
        <owl:hasValue>
          <CentralLeagueTeam rdf:ID="Carp">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Carp</rdfs:label>
          </CentralLeagueTeam>
        </owl:hasValue>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="JballTeam">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="BaseballTeam"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="CentralLeagueTeam"/>
          <owl:Class rdf:ID="PacificLeagueTeam"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="AllStarBaseballTeam">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="AllStarTeam"/>
          <owl:Class rdf:about="#BaseballTeam"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Team">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hires"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="#AllStarTeam">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="AllStarPlayer"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hires"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#AllStarPlayer">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#AllStarTeam"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#playsFor"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:ID="Player"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="AllStarBaseballPlayer">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#playsFor"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#AllStarBaseballTeam"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="BaseballPlayer">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#playsFor"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#BaseballTeam"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#BaseballTeam">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#BaseballPlayer"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hires"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#playsFor">
    <rdfs:domain rdf:resource="#Player"/>
    <rdfs:range rdf:resource="#Team"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hires">
    <owl:inverseOf rdf:resource="#playsFor"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hires</rdfs:label>
  </owl:ObjectProperty>
